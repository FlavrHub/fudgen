    ActNatural: () ->
        actions = [
            => @TwitchEar()
            => @TwitchEars()
            => @Blink()
            => @Walk()
            => @TwitchTail()
        ]
        return actions[Math.floor (Math.random() * actions.length)]()


    Blink: () ->
        @right_eye_closed.show()
        @left_eye_closed.show()
        @right_eye.hide()
        @left_eye.hide()
        _fn = =>
            @Unblink()
        setTimeout _fn, 200

    Unblink: () ->
        @right_eye_closed.hide()
        @left_eye_closed.hide()
        @right_eye.show()
        @left_eye.show()

    TwitchEars: () ->
        dir = if Math.random() < 0.5 then -1 else 1
        @TwitchEar @left_ear, dir
        @TwitchEar @right_ear, -1 * dir

    TwitchEar: (e,dir) ->
        if not e?
            e = if Math.random() < 0.5 then @left_ear else @right_ear
        console.log e
        if not dir?
            dir = if Math.random()<0.5 then 1 else -1
        e.animate 400
        .rotate (dir * 10),200,100
        .after ->
            e.animate 300
            .rotate 0,200,100

    TwitchTail: () ->
        @tail.animate 400
        .rotate if Math.random() < 0.5 then 15 else -15
        .after =>
            @tail.animate 300
            .rotate 0

    #walks to the given position
    #treat like center = (x,y)
    Walk: (x,y,duration) ->
        if not x? or not y?
            x = Math.random() * window.innerWidth - @gp.cx()
            y = Math.random() * window.innerHeight - @gp.cy()
        else
            x = x - @gp.cx()
            y = y - @gp.cy()
        if not duration?
            d = Math.sqrt x*x+y*y
            duration = 1000 * d * 1.0 / @pixels_per_second
        @gp.animate duration
        .move x, y
        @Run duration

    #changes position by the given amount, treat like center += (x,y)
    #takes duration time if given. else just uses base speed, see above
    WalkRelative: (x,y,duration) ->
        if not duration?
            d = Math.sqrt x*x+y*y
            duration = 1000 * d * 1.0 / @pixels_per_second
        @gp.animate duration
        .dmove x, y
        @Run duration

    Run: (duration) ->
        if not duration?
            duration = 1000
        i = parseInt(@leg_actions_per_second * duration * 1.0 / 1000)
        leg_speed = 1000 * 1.0 / @leg_actions_per_second
        @_run1 i,leg_speed/2

    _run1: (i,leg_speed) ->
        if i <= 0
            return
        @front_left_leg.animate leg_speed
        .rotate 20
        @front_right_leg.animate leg_speed
        .rotate -20
        @back_left_leg.animate leg_speed
        .rotate 20
        @back_right_leg.animate leg_speed
        .rotate -20
        .after =>
            @_run2 i,leg_speed

    _run2: (i,leg_speed) ->
        if i <= 0
            return
        @front_left_leg.animate leg_speed
        .rotate 0
        @front_right_leg.animate leg_speed
        .rotate 0
        @back_left_leg.animate leg_speed
        .rotate 0
        @back_right_leg.animate leg_speed
        .rotate 0
        .after =>
            @_run1 i-1,leg_speed

module.exports = Bunny
