    blink: () ->
        if Math.random() < 0.5
            console.log "left" #TODO
        else
            console.log "right"

    #walks to the given position
    #treat like center = (x,y)
    walkTo: (x,y,duration) ->
        if not duration?
            d = Math.sqrt x*x+y*y
            duration = 1000 * d * 1.0 / @pixels_per_second
        console.log "distance = #{duration}"
        @gp.animate(duration).move(x,y)
        @run(duration)

    #changes position by the given amount, treat like center += (x,y)
    #takes duration time if given. else just uses base speed, see above
    walkToRelative: (x,y,duration) ->
        if not duration?
            d = Math.sqrt x*x+y*y
            duration = 1000 * d * 1.0 / @pixels_per_second
        console.log "distance = #{duration}"
        @gp.animate(duration).dmove(x,y)
        @run(duration)

    run: (duration) ->
        if not duration?
            duration = 1000
        i = parseInt(@leg_actions_per_second * duration * 1.0 / 1000)
        leg_speed = 1000 * 1.0 / @leg_actions_per_second
        @_run1 i,leg_speed/2

    _run1: (i,leg_speed) ->
        if i <= 0
            return
        @front_left_leg.animate leg_speed
        .rotate 20
        @front_right_leg.animate leg_speed
        .rotate -20
        @back_left_leg.animate leg_speed
        .rotate 20
        @back_right_leg.animate leg_speed
        .rotate -20
        .after =>
            @_run2 i,leg_speed

    _run2: (i,leg_speed) ->
        if i <= 0
            return
        @front_left_leg.animate leg_speed
        .rotate 0
        @front_right_leg.animate leg_speed
        .rotate 0
        @back_left_leg.animate leg_speed
        .rotate 0
        @back_right_leg.animate leg_speed
        .rotate 0
        .after =>
            @_run1 i-1,leg_speed

module.exports = Bunny
